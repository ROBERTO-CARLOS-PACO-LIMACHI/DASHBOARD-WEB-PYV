                                    token: token,
                                    clientType: "WEB",
                                    timestamp: Date.now(),
                                };
                                ws.send(JSON.stringify(authMessage));
                            };

                            ws.onmessage = (event) => {
                                let parsedData;

                                try {
                                    // Intenta parsear directamente los datos del evento
                                    parsedData = JSON.parse(event.data);
                                    console.log('Mensaje recibido y parseado como JSON:', parsedData);

                                } catch (error) {
                                    // Si el primer parseo falla, es el mensaje especial.
                                    console.log('El mensaje no es un JSON directo. Intentando limpiar como string:', event.data);

                                    // Comprueba si el mensaje tiene el prefijo esperado
                                    if (event.data.startsWith('WEB_NOTIFICATION:')) {
                                        const jsonStringWithEscapes = event.data.substring('WEB_NOTIFICATION:'.length);

                                        try {
                                            // Elimina las barras invertidas y parsea el JSON anidado
                                            const cleanJsonString = jsonStringWithEscapes.replace(/\\"/g, '"');
                                            parsedData = JSON.parse(cleanJsonString);
                                            console.log('String limpiado y parseado con √©xito:', parsedData);
                                        } catch (innerError) {
                                            console.error("Error al parsear el JSON despu√©s de la limpieza:", innerError);
                                            return; // Salir si falla la limpieza
                                        }
                                    } else {
                                        console.error("Mensaje no reconocido:", event.data);
                                        return; // Mensaje no reconocido, salir
                                    }
                                }
                                

                                // Ahora, `parsedData` contiene el objeto correcto, independientemente del formato inicial.
